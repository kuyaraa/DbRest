<Window x:Class="DbRest.DobavPolzWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DbRest"
        mc:Ignorable="d"
        Title="DobavPolzWindow" Height="450" Width="800">
    <Grid>
        <Label Content="" HorizontalAlignment="Left" Margin="53,52,0,0" VerticalAlignment="Top"/>
        <Label Content="Имя пользователя" HorizontalAlignment="Left" Margin="37,48,0,0" VerticalAlignment="Top"/>
        <Label Content="Телефон" HorizontalAlignment="Left" Margin="42,89,0,0" VerticalAlignment="Top"/>
        <Label Content="Логин" HorizontalAlignment="Left" Margin="42,138,0,0" VerticalAlignment="Top"/>
        <Label Content="Пароль" HorizontalAlignment="Left" Margin="44,184,0,0" VerticalAlignment="Top"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="42,294,0,0" VerticalAlignment="Top" Height="33" Width="106" Click="Button_Click"/>
        <TextBox x:Name="TextBoxPassword" HorizontalAlignment="Left" Margin="116,182,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" Height="29" TextChanged="TextBoxPassword_TextChanged"/>
        <TextBox x:Name="TextBoxLogin" HorizontalAlignment="Left" Margin="116,138,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="208" Height="26" TextChanged="TextBoxLogin_TextChanged"/>
        <TextBox x:Name="TextBoxTelefon" HorizontalAlignment="Left" Margin="116,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" Height="26" TextChanged="TextBoxTelefon_TextChanged"/>
        <TextBox x:Name="TextBoxName" HorizontalAlignment="Left" Margin="153,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160" Height="22" TextChanged="TextBoxName_TextChanged"/>
    </Grid>
</Window>
