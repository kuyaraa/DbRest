<Window x:Class="DbRest.IsmenitWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DbRest"
        mc:Ignorable="d"
        Title="IsmenitWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Имя пользователя" HorizontalAlignment="Left" Margin="82,40,0,0" VerticalAlignment="Top"/>
        <Label Content="Телефон" HorizontalAlignment="Left" Margin="97,96,0,0" VerticalAlignment="Top"/>
        <Label Content="Логин" HorizontalAlignment="Left" Margin="103,160,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxName" HorizontalAlignment="Left" Margin="198,53,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TextBoxName_TextChanged"/>
        <TextBox x:Name="TextBoxPhone" HorizontalAlignment="Left" Margin="177,109,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TextBoxPhone_TextChanged"/>
        <TextBox x:Name="TextBoxLogin" HorizontalAlignment="Left" Margin="177,160,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TextBoxLogin_TextChanged"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="125,301,0,0" VerticalAlignment="Top" Height="32" Width="133" Click="Button_Click"/>

    </Grid>
</Window>
